<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="550.0" prefWidth="1000.0" style="-fx-border-style: null;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gomarket.supermarket.controllers.StatisticsController">
   <children>
      <Pane prefHeight="82.0" prefWidth="1000.0" style="-fx-background-color: #7952B3;">
         <children>
            <Label layoutX="412.0" layoutY="-2.0" prefHeight="82.0" prefWidth="168.0" text="Statistics" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Circle fill="WHITE" layoutX="62.0" layoutY="40.0" radius="33.0" stroke="BLACK" strokeType="INSIDE" />
            <ImageView fitHeight="74.0" fitWidth="66.0" layoutX="21.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Images/back.png" />
               </image>
            </ImageView>
            <Button layoutX="30.0" layoutY="2.0" mnemonicParsing="false" onAction="#goBack" prefHeight="76.0" prefWidth="66.0" style="-fx-background-color: none;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children></Pane>
      <TextArea fx:id="textArea" editable="false" layoutX="455.0" layoutY="110.0" prefHeight="357.0" prefWidth="506.0" wrapText="true">
         <font>
            <Font size="18.0" />
         </font></TextArea>
      <ComboBox fx:id="queryMenu" layoutX="34.0" layoutY="110.0" onAction="#executeQuery" prefHeight="54.0" prefWidth="377.0" promptText="Select A Query">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
   </children>
</AnchorPane>
